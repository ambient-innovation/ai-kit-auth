{
  "name": "ai-kit-auth",
  "version": "0.1.0",
  "description": "Out of the box login capability in combination with django-ai-kit-auth",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "quiet": false
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{tsx,ts}'",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.ambient-innovation.com/ai/ai.kit/authentication.git"
  },
  "keywords": [
    "login",
    "react",
    "material-ui"
  ],
  "author": "Ambient Innovation",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "@material-ui/core": "^4.9.5",
    "axios": "^0.19.0",
    "history": "^4.0.0",
    "react": "^16.8.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/gitlab": "^6.0.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/axios": "^0.14.0",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.2",
    "@types/react": "^16.9.20",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@material-ui/core": "^4.9.5",
    "axios": "^0.19.2",
    "history": "^4.10.1",
    "react": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "npm-watch": "^0.6.0",
    "react-dom": "^16.12.0",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "tagFormat": "react_v${version}",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "scope": "!*react*",
              "release": false
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/gitlab",
        {
          "gitlabUrl": "https://gitlab.ambient-innovation.com/",
          "assets": []
        }
      ],
      "@semantic-release/npm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
